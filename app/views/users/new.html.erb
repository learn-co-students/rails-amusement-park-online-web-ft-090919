<%= form_for :user, url: '/users' do |f| %>
    Name: <%= f.text_field :name %>
    Password: <%= f.password_field :password %>
    Current level of nausea: <%= f.number_field :nausea %>
    Current level of happiness: <%= f.number_field :happiness %>
    Number of tickets: <%= f.number_field :tickets %>
    Height: <%= f.number_field :height %>
    Admin?: <%= f.check_box :admin %>
    <%= f.submit "Create User" %>
  <% end %>

